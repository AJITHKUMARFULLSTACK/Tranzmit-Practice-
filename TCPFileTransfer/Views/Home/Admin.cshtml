
@{
    ViewBag.Title = "Admin";
}
<h1> WELCOME YOU CAN TRANSFER FILE </h1>
h2>File Transfer</h2>

@using (Html.BeginForm("UploadFile", "FileTransfer", FormMethod.Post, new { enctype = "multipart/form-data" }))
{
    <div class="form-group">
        <label for="file">Choose file:</label>
        <input type="file" name="file" id="file" class="form-control" />
    </div>
    <button type="submit" class="btn btn-primary">Upload</button>
}

